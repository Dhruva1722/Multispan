<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AccountFragment">





        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="20dp"
            android:layout_gravity="center_horizontal">

            <LinearLayout
                android:id="@+id/lyProfileID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="6dp"
                android:padding="7dp"
                android:background="#DEEFF8">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Profile"
                    android:textSize="15sp"
                    android:drawablePadding="10dp"
                    android:drawableLeft="@drawable/baseline_account_circle_24"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lyAboutUsID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="6dp"
                android:padding="7dp"
                android:background="#DEEFF8">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="AboutUs"
                    android:drawablePadding="10dp"
                    android:drawableLeft="@drawable/baseline_quiz_24"
                    android:textSize="15sp"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lyContactUsID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="6dp"
                android:padding="7dp"
                android:background="#DEEFF8">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ContactUs"
                    android:drawableLeft="@drawable/baseline_contact_page_24"
                    android:drawablePadding="10dp"
                    android:textColor="@color/black"
                    android:textSize="15sp"/>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/lyLogoutID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="6dp"
                android:padding="7dp"
                android:background="#DEEFF8">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Logout"
                    android:drawableLeft="@drawable/baseline_logout_24"
                    android:drawablePadding="10dp"
                    android:textSize="15sp"
                    android:textColor="@color/black" />

            </LinearLayout>
        </LinearLayout>




</FrameLayout>